{
  "name": "starter",
  "version": "1.0.0",
  "description": "Webpack basic configuration.",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config config/webpack.dev.js --env.ENVIRONMENT=development",
    "start:prod": "webpack-dev-server --config config/webpack.prod.js --env.ENVIRONMENT=production",
    "build": "webpack --config config/webpack.prod.js --env.ENVIRONMENT=production",
    "check-staged": "lint-staged",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "npm run lint:eslint && npm run lint:css",
    "lint:css": "stylelint src/**/*.css",
    "lint:eslint": "eslint src/ --ext .js --ext .jsx",
    "prettier": "prettier --write src/**/*.{js,jsx}"
  },
  "keywords": [],
  "author": "DC",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.2.2",
    "babel-loader": "^8.3.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "css-loader": "^4.3.0",
    "dotenv": "^8.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^7.6.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-plugin-redux-saga": "^1.1.3",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.2.2",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "style-loader": "^1.3.0",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.1"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.13.1"
  }
}
